{
    "compilerOptions": {
        "outDir": "./dist",
        "allowJs": true,
        "target": "es5",
        "moduleResolution": "node",
        // "strict": true,
        // "noUnusedLocals": true,
        "removeComments": true,
        "esModuleInterop": true,
        // "noImplicitReturns": true,
        // "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        // "emitDecoratorMetadata": true,
        "lib": ["es5","dom","es6"],
        "module": "commonjs",
    },
    "include": [
        "./src/**/*",
        "./migrations"
    ],
        "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}